{
  "name": "brutus",
  "version": "1.1.4",
  "description": "A tool for creating a cli which uses cli options and package.json options and merge them into default options ",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "mocha spec",
    "cover": "istanbul cover _mocha -x \"spec/**/*.js\" -- spec/",
    "codecov": "npm run cover && codecov < coverage/lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kt3k/brutus.git"
  },
  "keywords": [
    "cli",
    "command",
    "package.json",
    "options"
  ],
  "author": "Yoshiya Hinosawa <stibium121@gmail.com> (https://twitter.com/kt3k)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kt3k/brutus/issues"
  },
  "homepage": "https://github.com/kt3k/brutus#readme",
  "dependencies": {
    "es6-promise": "^3.0.2",
    "minimist": "^1.2.0",
    "pkg-config": "^1.1.0",
    "subclassjs": "^1.3.0"
  },
  "devDependencies": {
    "chai": "^3.4.1",
    "codecov.io": "^0.1.6",
    "istanbul": "^0.4.0",
    "mocha": "^2.3.4"
  },
  "dummycommand": {
    "foo": "bar"
  }
}
